{
  "name": "miya-protocol",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "frontend",
    "sdk"
  ],
  "scripts": {
    "start:frontend": "cd frontend && npm start",
    "build:frontend": "cd frontend && npm run build",
    "start:sdk": "cd sdk && npm start",
    "build:sdk": "cd sdk && npm run build",
    "test": "npm run test:frontend && npm run test:sdk",
    "test:frontend": "cd frontend && npm test",
    "test:sdk": "cd sdk && npm test",
    "install:all": "npm install && npm run install:frontend && npm run install:sdk",
    "install:frontend": "cd frontend && npm install",
    "install:sdk": "cd sdk && npm install"
  },
  "devDependencies": {
    "typescript": "^4.9.5"
  }
} 